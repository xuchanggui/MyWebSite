	<script type="text/javascript">
		function on_wnd_resize(){
			var rh = 0;
			var w = $(".main-content").width();
			var lw = $(".left-menu").width();
			$(".right").find(">div").each(function(i, item){
				rh += $(item).height();
			});

			
			$(".right").height(rh);
			$(".right").width(w-lw - 10);
			var lh = $(".left-menu").height();
			var rh = $(".right").height();
			if(lh < rh){
				$(".left-menu").height(rh);
			}

			$(".left-menu").height(rh);
			$("#nav").width(lw + $(".right").width());
			$(".main-content").height($(".left-menu").height);
		}
		
		(function(){
			on_wnd_resize();
			$(window).resize(function(){
				on_wnd_resize();
			});
			
		})();       

	</script>
	


